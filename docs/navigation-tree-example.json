        },
        "DOMAIN_EVENT": {
          "fontSize": "11px",
          "color": "#722ed1"
        }
      },
      "actions": [
        {
          "type": "button",
          "label": "查看详情",
          "icon": "fa fa-eye",
          "level": "link",
          "size": "sm",
          "onEvent": {
            "click": {
              "actions": [
                {
                  "actionType": "dialog",
                  "dialog": {
                    "title": "查看详情",
                    "body": {
                      "type": "form",
                      "body": [
                        {
                          "type": "static",
                          "name": "nodeId",
                          "label": "节点ID"
                        },
                        {
                          "type": "static",
                          "name": "nodeName",
                          "label": "节点名称"
                        },
                        {
                          "type": "static",
                          "name": "nodeType",
                          "label": "节点类型"
                        },
                        {
                          "type": "json",
                          "name": "nodeData",
                          "label": "节点数据"
                        }
                      ]
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "type": "button",
          "label": "编辑",
          "icon": "fa fa-edit",
          "level": "link",
          "size": "sm",
          "onEvent": {
            "click": {
              "actions": [
                {
                  "actionType": "dialog",
                  "dialog": {
                    "title": "编辑节点",
                    "body": {
                      "type": "form",
                      "api": "/api/navigation-tree/update",
                      "body": [
                        {
                          "type": "hidden",
                          "name": "nodeId"
                        },
                        {
                          "type": "input-text",
                          "name": "nodeName",
                          "label": "节点名称",
                          "required": true
                        },
                        {
                          "type": "select",
                          "name": "nodeType",
                          "label": "节点类型",
                          "options": [
                            {"label": "项目", "value": "PROJECT"},
                            {"label": "子域", "value": "SUBDOMAIN"},
                            {"label": "聚合", "value": "AGGREGATE"},
                            {"label": "实体", "value": "ENTITY"},
                            {"label": "值对象", "value": "VALUE_OBJECT"},
                            {"label": "领域服务", "value": "DOMAIN_SERVICE"},
                            {"label": "领域事件", "value": "DOMAIN_EVENT"}
                          ],
                          "required": true
                        },
                        {
                          "type": "input-text",
                          "name": "nodeIcon",
                          "label": "节点图标"
                        }
                      ]
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "type": "button",
          "label": "删除",
          "icon": "fa fa-trash",
          "level": "link",
          "size": "sm",
          "confirmText": "确定要删除这个节点吗？",
          "onEvent": {
            "click": {
              "actions": [
                {
                  "actionType": "ajax",
                  "api": {
                    "url": "/api/navigation-tree/delete",
                    "method": "DELETE",
                    "data": {
                      "nodeId": "${nodeId}"
                    }
                  }
                },
                {
                  "actionType": "reload",
                  "target": "navigation-tree"
                }
              ]
            }
          }
        }
      ],
      "onEvent": {
        "change": {
          "actions": [
            {
              "actionType": "setValue",
              "componentId": "selected-node-info",
              "args": {
                "value": "${event.data.value}"
              }
            }
          ]
        },
        "expand": {
          "actions": [
            {
              "actionType": "setValue",
              "componentId": "expanded-nodes",
              "args": {
                "value": "${event.data.expandedKeys}"
              }
            }
          ]
        }
      }
    },
    "themeConfig": {
      "primaryColor": "#1890ff",
      "successColor": "#52c41a",
      "warningColor": "#faad14",
      "dangerColor": "#f5222d",
      "infoColor": "#13c2c2",
      "textColor": "#333333",
      "borderColor": "#d9d9d9",
      "backgroundColor": "#ffffff",
      "hoverColor": "#f5f5f5",
      "selectedColor": "#e6f7ff"
    },
    "eventHandlers": [
      {
        "eventType": "nodeClick",
        "handlerType": "NAVIGATION",
        "handlerConfig": {
          "navigationType": "ROUTE",
          "routePath": "/ddd-platform/${nodeType}/${nodeId}",
          "routeParams": {
            "nodeId": "${nodeId}",
            "nodeType": "${nodeType}",
            "nodeData": "${nodeData}"
          }
        }
      },
      {
        "eventType": "nodeExpand",
        "handlerType": "API_CALL",
        "handlerConfig": {
          "apiEndpoint": "/api/navigation-tree/expand",
          "method": "POST",
          "requestData": {
            "nodeId": "${nodeId}",
            "expanded": true
          }
        }
      },
      {
        "eventType": "nodeCollapse",
        "handlerType": "API_CALL",
        "handlerConfig": {
          "apiEndpoint": "/api/navigation-tree/collapse",
          "method": "POST",
          "requestData": {
            "nodeId": "${nodeId}",
            "expanded": false
          }
        }
      }
    ]
  }
}
